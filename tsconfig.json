{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./output",
    "rootDir": "./",
    "baseUrl": ".",
    "paths": {
      "@fixtures/*": ["src/fixtures/*"]
    }
  },
  "include": [
    // "tests/**/*.ts",
    // "tests-examples/**/*.ts",
    "./**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}